    function EEtoHE ConvertElectricalEnergyToHeatEnergy
    function exportHE2 ExportHeatEnergy1
	function exportHE3 ExportHeatEnergy1
	function exportHE4 ExportHeatEnergy1
    
    flow importEE storeEE Electrical
